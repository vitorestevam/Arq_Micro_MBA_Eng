{
    "name": "jdbc-sink-orders-demo-mysql",
    "config": {
        "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
        "tasks.max": "1",
        "topics": "demo-orders",
        "connection.url": "jdbc:mysql://mysql:3306/demo_db?useSSL=false&allowPublicKeyRetrieval=true",
        "connection.user": "demo_user",
        "connection.password": "demo_password",
        "connection.driver.class": "com.mysql.cj.jdbc.Driver",
        "insert.mode": "upsert",
        "pk.mode": "record_value",
        "pk.fields": "order_id",
        "auto.create": "false",
        "auto.evolve": "false",
        "table.name.format": "orders",
        "value.converter": "io.confluent.connect.avro.AvroConverter",
        "value.converter.schema.registry.url": "http://schema-registry:8081"
    }
}
